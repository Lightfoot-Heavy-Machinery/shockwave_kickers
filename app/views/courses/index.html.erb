<%= button_to(
        "Log Out",
        destroy_user_session_path,
        method: :delete
      ) %>
<p style="color: green"><%= notice %></p>
<h1>Courses</h1>

<div id="courses">
  <% @courses.each do |course| %>
    <%= render course %>
    <p>
      <%= link_to "Show this course", course %>
    </p>
  <% end %>
</div>

<%= link_to "New course", new_course_path %>
