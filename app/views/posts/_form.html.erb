<%= form_with url: '/posts', method: :get do |form| %>

  <div style = "text-align: center">
    <strong class = "message">Drag and Drop OR Click "Save" to Upload</strong><br><br>
      <%= form.file_field :file %>
  </div>

  <div style= "text-align: center">
    <%= form.submit %>
  </div>
<% end %>